text.usetex: True
font.family: serif
font.size: 10
# figure.dpi: 150
figure.figsize: 3,2
# Tableau colorblind 10 palette
axes.prop_cycle: cycler('color', ['006BA4', 'FF800E', 'ABABAB', '595959', '5F9ED1', 'C85200', '898989', 'A2C8EC', 'FFBC79', 'CFCFCF'])
